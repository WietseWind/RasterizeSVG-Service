  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.21.1",
    "helmet": "^8.0.0",
    "ioredis": "^5.4.1",
    "ip-address": "^10.0.1",
    "node-fetch": "^3.3.2",
    "pm2": "^5.4.2",
    "sharp": "^0.33.5"
  }
}
